= provide(:title, "All Clients")

.row
  .eight.columns
  %h1 Single Sign On Application Clients
  %table.twelve
    %thead
      %tr
        %th Name
        %th App ID
        %th App Access Token
    %tbody
      - @clients.each do |client|
        %tr
          %td 
            = client.name
          %td
            = client.app_id
          %td
            = client.app_access_token
